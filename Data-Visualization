        "NSEP_list = pd.DataFrame({'start time': NSEP_list_['start time'].apply(lambda x: x[:-4]),\n",
        "  'flare class': NSEP_list_['flare class']\n",
        "}) # Removing .000 from NSEP_list_"
      ],
      "metadata": {
        "id": "ZTyKor63BYZL"
