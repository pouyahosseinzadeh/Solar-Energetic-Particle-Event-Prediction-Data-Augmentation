      "source": [
        "def convertStr(s):\n",
        "  my_x = s[-4]\n",
        "  if(int(my_x)<5):\n",
        "     return s[:-4] + '0' + ':00'\n",
        "  else:\n",
        "     return s[:-4] + '5' + ':00'\n"
      ],
      "metadata": {
        "id": "TmvKA4B7l0t9"
