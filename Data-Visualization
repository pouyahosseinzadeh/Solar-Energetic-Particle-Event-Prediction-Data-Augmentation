        "### Gaussian"
      ],
      "metadata": {
        "id": "JMq0ucPaWUSU"
      }
    },
    {
      "cell_type": "code",
      "execution_count": 123,
      "metadata": {
        "id": "xRmT6ABui1fA"
      },
      "outputs": [],
      "source": [
        "def DA_Jitter(X, sigma, p):  # 0.00089  or  0.03  or 0.0002\n",
        "    R = np.random.RandomState(p)\n",
        "    myNoise = R.normal(loc=0, scale=sigma, size=X.shape)\n",
        "    return X+myNoise"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 124,
      "metadata": {
        "id": "Xp13jI1Y93xL"
      },
      "outputs": [],
      "source": [
        "SEP_df_2 = DA_Jitter(SEP_df_1, 0.0002, 2)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
