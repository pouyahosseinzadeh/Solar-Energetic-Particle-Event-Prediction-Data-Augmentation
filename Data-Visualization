        "nsep_flare_list = NSEP_list['start time']"
      ],
      "metadata": {
        "id": "fujbfZtOEsEz"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "my_event_indices = [] # finding SEP events in NSEP_list\n",
        "for i, j in enumerate(nsep_flare_list):\n",
        "  if j in sep_flare_list.tolist():\n",
        "    my_event_indices.append(i)"
      ],
      "metadata": {
        "id": "y9lQJs_k_-az"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
